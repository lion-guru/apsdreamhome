<IfModule mod_rewrite.c>
    RewriteEngine On
    
    # Serve existing files/directories directly
    RewriteCond %{REQUEST_FILENAME} -f [OR]
    RewriteCond %{REQUEST_FILENAME} -d
    RewriteRule ^ - [L]
    
    # Route everything else through index.php
    RewriteRule ^(.*)$ index.php [L]
</IfModule>
