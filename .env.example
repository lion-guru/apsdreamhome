# APS Dream Home â€“ Environment Template
# Copy this file to .env and fill in real values

# APP
APP_NAME="APS Dream Home"
APP_ENV=local
APP_DEBUG=true
APP_URL=http://localhost:8000

# DATABASE
DB_HOST=localhost
DB_PORT=3306
DB_NAME=apsdreamhome
DB_USER=root
DB_PASSWORD=your_mysql_password
DB_PASS=your_mysql_password

# MAIL (SMTP)
MAIL_DRIVER=smtp
MAIL_HOST=your_smtp_host
MAIL_PORT=your_smtp_port
MAIL_USERNAME=your_smtp_username
MAIL_PASSWORD=your_smtp_password
MAIL_ENCRYPTION=your_smtp_encryption
MAIL_FROM_ADDRESS=your_from_address
MAIL_FROM_NAME="Your App Name"
MAIL_REPLY_TO=your_reply_to_address
MAIL_ADMIN=your_admin_email

# FILE UPLOAD
UPLOAD_MAX_SIZE=5M
ALLOWED_EXTENSIONS=jpg,jpeg,png,gif,pdf

# API KEYS (optional)
GOOGLE_MAPS_API_KEY=your_google_maps_key
RECAPTCHA_SITE_KEY=your_recaptcha_site_key
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=your_openrouter_model_name

# SESSION & CACHE
SESSION_DRIVER=file
CACHE_DRIVER=file

# SECURITY
CSRF_TOKEN_NAME=_token
SESSION_LIFETIME=120

# LOGGING
LOG_LEVEL=debug
LOG_CHANNEL=daily

# WHATSAPP (optional)
WHATSAPP_PHONE=your_whatsapp_phone
WHATSAPP_COUNTRY_CODE=your_whatsapp_country_code
WHATSAPP_API_PROVIDER=your_whatsapp_api_provider
WHATSAPP_BUSINESS_ACCOUNT_ID=your_whatsapp_business_account_id
WHATSAPP_ACCESS_TOKEN=your_whatsapp_access_token
WHATSAPP_WEBHOOK_VERIFY_TOKEN=your_whatsapp_webhook_verify_token
