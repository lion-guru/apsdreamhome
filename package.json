{
  "name": "aps-dream-home",
  "version": "2.0.0",
  "description": "Professional Real Estate Platform - APS Dream Home (Enhanced)",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "php -S localhost:8000 -t .",
    "start": "run-p dev serve",
    "lint": "eslint assets/js/**/*.js --fix",
    "format": "prettier --write assets/js/**/*.js assets/css/**/*.css",
    "test": "echo \"Setting up test suite...\" && exit 0",
    "optimize": "node optimize.js"
  },
  "keywords": [
    "real-estate",
    "property-management",
    "php",
    "vite",
    "pwa",
    "modern-web",
    "performance"
  ],
  "author": "APS Dream Home Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "aos": "^2.3.4",
    "bootstrap": "^5.3.3",
    "jquery": "^3.7.1",
    "vite-plugin-pwa": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.1",
    "markdownlint-cli": "^0.45.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "vite": "^5.4.21"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
