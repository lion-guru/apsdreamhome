  "main": "optimize.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhaysingh3007aps/apsdreamhome.git"
  },
  "bugs": {
