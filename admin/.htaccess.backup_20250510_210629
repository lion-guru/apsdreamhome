# Admin Directory .htaccess
RewriteEngine On
Options -Indexes

# Redirect to login if not authenticated
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
# Check if login.php exists and use correct path
# यदि login.php मूल प्रोजेक्ट फोल्डर में है
RewriteRule ^(.*)$ ../login.php [L,QSA]

# Prevent direct access to sensitive files - Apache 2.4 syntax
<FilesMatch "^(config\.php|\.htaccess)$">
    Require all denied
</FilesMatch>

# Prevent direct access to sensitive files
<FilesMatch "^(config\.php|\.htaccess)$">
    Order Allow,Deny
    Deny from all
</FilesMatch>

# Prevent direct access to sensitive files
<FilesMatch "^(config\.php|\.htaccess)$">
    Order Allow,Deny
    Deny from all
</FilesMatch>

